# 服务配置
SERVER_PORT=3050

# 环境标识
CURRENT_ENV=dev

# JWT 配置
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=2h

# MongoDB 配置（可选，也可以在 config.json 中配置）
MONGODB_HOST=127.0.0.1
MONGODB_PORT=27017
MONGODB_DATABASE=node-template
MONGODB_USERNAME=
MONGODB_PASSWORD=

# Redis 配置（可选）
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=

# 日志配置
LOG_LEVEL=info
